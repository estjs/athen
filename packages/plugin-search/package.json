{
  "name": "@athen/plugin-search",
  "version": "0.1.0",
  "description": "Search plugin for Athen documentation framework",
  "keywords": [
    "athen",
    "plugin",
    "search",
    "flexsearch"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./src/client/index.ts",
      "import": "./src/client/index.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "athen": "workspace:*"
  },
  "optionalDependencies": {
    "algoliasearch": "^5.48.0",
    "flexsearch": "^0.8.212",
    "glob-to-regexp": "^0.4.1",
    "markdown-it": "^14.1.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.9",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
