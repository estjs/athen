{
  "name": "athen",
  "version": "0.0.6",
  "description": "Vite & Essor document framework",
  "type": "module",
  "keywords": [
    "md",
    "mdx",
    "essor",
    "document",
    "vite"
  ],
  "license": "MIT",
  "homepage": "https://github.com/estjs/athen#readme",
  "bugs": "https://github.com/estjs/athen/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/athen.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "bin",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "bin": {
    "athen": "bin/athen.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit -p ./tsconfig.json"
  },
  "dependencies": {
    "@svgr/core": "6.3.1",
    "b-tween": "^0.3.3",
    "babel-plugin-essor": "^0.0.8",
    "copy-to-clipboard": "3.3.3",
    "essor": "^0.0.8",
    "essor-router": "^0.0.7",
    "flexsearch": "0.6.32",
    "fs-extra": "^11.2.0",
    "plugins-mdx": "workspace:*"
  },
  "devDependencies": {
    "@babel/types": "^7.25.2",
    "@estjs/tools": "^0.1.28",
    "@iconify-json/carbon": "^1.1.37",
    "@polka/compression": "^1.0.0-next.25",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "cac": "^6.7.14",
    "dayjs": "^1.11.12",
    "fast-glob": "^3.3.2",
    "lodash-es": "^4.17.21",
    "polka": "^1.0.0-next.25",
    "sass": "^1.77.8",
    "sirv": "^2.0.4",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unocss": "^0.61.9",
    "vite": "^5.4.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-inspect": "^0.8.5"
  }
}
