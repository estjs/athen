{
  "name": "athen",
  "version": "0.0.7-beta.3",
  "description": "Vite & Essor document framework",
  "type": "module",
  "keywords": [
    "md",
    "mdx",
    "essor",
    "document",
    "vite"
  ],
  "license": "MIT",
  "homepage": "https://github.com/estjs/athen#readme",
  "bugs": "https://github.com/estjs/athen/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estjs/athen.git"
  },
  "author": "jiangxd <jiangxd2016@gmail.com>",
  "funding": "https://github.com/sponsors/estjs",
  "files": [
    "bin",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "bin": {
    "athen": "bin/athen.js"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit -p ./tsconfig.json"
  },
  "dependencies": {
    "@athen/plugin-analytics": "workspace:*",
    "@athen/plugin-mdx": "workspace:*",
    "@athen/plugin-search": "workspace:*",
    "@iconify-json/carbon": "^1.2.16",
    "@polka/compression": "^1.0.0-next.25",
    "@svgr/core": "8.1.0",
    "b-tween": "^0.3.3",
    "babel-plugin-essor": "0.0.15-beta.8",
    "cac": "^6.7.14",
    "copy-to-clipboard": "3.3.3",
    "dayjs": "^1.11.19",
    "essor": "0.0.15-beta.8",
    "essor-router": "^0.0.16-beta.7",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "lodash-es": "^4.17.22",
    "polka": "^1.0.0-next.28",
    "sirv": "^3.0.2",
    "unocss": "^66.5.12",
    "vite": "^7.3.1",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-inspect": "^11.3.3"
  },
  "devDependencies": {
    "@babel/types": "^7.28.5",
    "@estjs/tools": "^0.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.6",
    "sass": "^1.97.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
